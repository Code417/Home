aliasout () 
{ 
    cat > /dev/null  <<EOF
###"If your hash bang is /bin/sh then you should care about what POSIX says. type and hash's exit codes aren't terribly well defined by POSIX,"
###"and hash is seen to exit successfully when the command doesn't exist (haven't seen this with type yet)." 
###"command's exit status is well defined by POSIX, so that one is probably the safest to use."
###"'"> command -v foo >/dev/null 2>&1 || { echo >&2 "I require foo but it's not installed.  Aborting."; exit 1; }
###"'"> type foo >/dev/null 2>&1 || { echo >&2 "I require foo but it's not installed.  Aborting."; exit 1; }
###"'"> hash foo 2>/dev/null || { echo >&2 "I require foo but it's not installed.  Aborting."; exit 1; }
EOF

    cat > ~/$(basename "$BASH_SOURCE")  <<EOF
#!/bin/bash
##### "This is a dynamically self-generated file."
##### "To add a new entry add a new alias, "
##### "then use the reload command."
EOF

    alias >> ~/$(basename "$BASH_SOURCE");
    echo -en "\n\n\n\n" >> ~/$(basename "$BASH_SOURCE");
    echo -en "\n\n\n\n" >> ~/$(basename "$BASH_SOURCE");
    declare -f aliasout >> ~/$(basename "$BASH_SOURCE");
    echo aliasout >> ~/$(basename "$BASH_SOURCE")
}
aliasout
